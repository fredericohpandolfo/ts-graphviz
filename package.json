{
  "name": "ts-graphviz",
  "private": true,
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:parser": "ts-node-esm ./scripts/generate-parser.ts ./grammar/dot.peggy ./packages/parser/src/dot.peggy.js",
    "prebuild": "yarn build:parser",
    "build": "yarn workspace @ts-graphviz/dot-type build && yarn workspace @ts-graphviz/dot-attribute build && yarn workspace @ts-graphviz/dot-ast build && yarn workspace @ts-graphviz/model build && yarn workspace @ts-graphviz/parser build && yarn workspace @ts-graphviz/renderer build && yarn workspace ts-graphviz build && yarn workspace @ts-graphviz/react build && yarn workspace @ts-graphviz/node build",
    "test": "jest",
    "format": "eslint --ext ts,tsx . --fix && prettier --plugin=./config/prettier/peggy.json --write './**/*.(json|yaml|ts|tsx|peggy)'",
    "lint": "eslint --ext ts,tsx .",
    "create-img:packages-dependency-diagram": "ts-node-esm scripts/create-packages-dependency-diagram.ts ./img/packages-dependency.svg",
    "create-img:parser-state-machine-diagram": "ts-node-esm scripts/create-state-machine-diagram.ts ./packages/parser/img/state-machine.svg",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@hpcc-js/wasm": "^1.14.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^27.5.1",
    "@types/jest-specific-snapshot": "^0.5.5",
    "@types/node": "*",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-reconciler": "^0.18.0",
    "@types/svgo": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fs-extra": "^9.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-graphviz": "^0.5.0",
    "jest-snapshot-serializer-raw": "^1.2.0",
    "jest-specific-snapshot": "^5.0.0",
    "lerna": "^3.22.1",
    "peggy": "^2.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-pegjs": "^0.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.73.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "svgo": "^2.3.1",
    "ts-dedent": "^2.1.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^7.0.2",
