    "postbuild": "prettier --write ./lib/index.{js,cjs,d.ts}"
  },
  "peerDependencies": {
    "@hpcc-js/wasm": "^0.3.13",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@ts-graphviz/model": "^0.0.0",
    "ts-graphviz": "^0.16.0"
  },
  "peerDependenciesMeta": {
    "@hpcc-js/wasm": {
      "optional": true
    }
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-reconciler": "^0.25.1"
