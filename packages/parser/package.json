{
  "name": "@ts-graphviz/parser",
  "version": "0.1.0",
  "description": "Graphviz dot language parser for ts-graphviz.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "author": "kamiazya <yuki@kamiazya.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-graphviz/parser.git"
  },
  "bugs": {
    "url": "https://github.com/ts-graphviz/parser/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/kamiazya"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "graphviz",
    "dot",
    "parser"
  ],
  "scripts": {
    "build:peg": "pegjs --plugin ts-pegjs -o src/dot.pegjs.ts grammar/dot.pegjs",
    "prebuild": "yarn build:peg",
    "build": "rollup -c && prettier --write './lib/index.d.ts'",
    "pretest": "yarn build:peg",
    "test": "jest",
    "format": "eslint -c .eslintrc.json --ext ts src --fix && prettier --write './src/**/*.ts'",
    "lint": "eslint -c .eslintrc.json --ext ts src"
  },
  "dependencies": {
    "ts-graphviz": "^0.15.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/pegjs": "^0.10.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "pegjs": "^0.10.0",
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-dedent": "^2.1.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-pegjs": "^0.3.1",
    "typescript": "^4.2.4"
  }
}
