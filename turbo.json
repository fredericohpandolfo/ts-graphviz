{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "dependsOn": [],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts", "src/**/*.tsx", "rollup.config.ts", "tsconfig.json"],
      "outputs": ["lib/**"]
    },
    "@ts-grapphviz/parser#build": {
      "dependsOn": ["^build"],
      "inputs": ["peggy/**/*", "scripts/**/*", "src/**/*.ts", "src/**/*.tsx", "rollup.config.ts", "tsconfig.json"],
      "outputs": ["src/parse.js", "lib"]
    },
    "test": {
      "outputs": []
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
