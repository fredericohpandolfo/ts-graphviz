{
  "compilerOptions": {
    "importHelpers": true,
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "allowJs": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "outDir": "lib",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true
  }
}
